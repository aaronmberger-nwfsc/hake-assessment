\section{Reference points}
\subsection{}

%%%%%%%%%%%%%%%%%%%%%%% 
\frame{\frametitle{Background on reference points}
%\begin{tcolorbox}[colback=green!5,colframe=green!40!black,title=Treaty definitions]
\begin{block}{Treaty definitions}
\bi
\item ``F-40 percent'' means a fishing mortality rate that would reduce the 
  biomass, calculated on a per recruit basis, to 40 percent of what it would 
  have been in the absence of fishing mortality;
\item ``40/10 adjustment'' means an adjustment to the overall total allowable 
  catch (TAC) that is triggered when the biomass falls below 40 percent of its 
  unfished level. This adjustment reduces the TAC on a straight-line basis from 
  the 40 percent level such that the TAC would equal zero when the stock is at
  10 percent of its unfished level;
\ei
\end{block}

\begin{block}{Interpretation by the JTC}
\bi
\item JTC has been calculating $\Bforty$ and $\Bten$ reference points in terms of
  ``relative spawning biomass'':
  the ratio of the beginning-of-the-year female spawning biomass to
  the unfished equilibrium female spawning biomass ($B_0$).
\ei
\end{block}
}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% 

\frame{\frametitle{Calculation of $B_0$}
\begin{columns}
  \begin{column}{0.5\textwidth}
    \bi
      \item $B_0$ = $R_0  \times$ spawning biomass per recruit
      \item $R_0$ is an estimated quantity linked to average recruitment
      \item spawning biomass per recruit = per-recruit-numbers-at-age $\times$
        fecundity, summed across all ages
      \item fecundity = weight-at-age $\times$ maturity-at-age
    \ei
  \end{column}
  \begin{column}{0.5\textwidth}
  <<SPR.illustration.1, fig.height=7, fig.width=6, out.width='0.85\\columnwidth'>>=
    make.SPR.illustration(base.model, show.fished=FALSE, show.legend=FALSE)
  @ 
  \end{column}
\end{columns}
}


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% 

\frame{\frametitle{Calculation of Spawning Potential Ratio (SPR)}
\begin{columns}
  \begin{column}{0.5\textwidth}
    \bi
      \item $B_0$ = $R_0  \times$ spawning biomass per recruit
      \item $R_0$ is an estimated quantity linked to average recruitment
      \item spawning biomass per recruit = per-recruit-numbers-at-age $\times$
        fecundity, summed across all ages
      \item fecundity = weight-at-age $\times$ maturity-at-age
    \ei
  \end{column}
  \begin{column}{0.5\textwidth}
  <<SPR.illustration.2, fig.height=7, fig.width=6, out.width='0.85\\columnwidth'>>=
    make.SPR.illustration(base.model, show.fished=TRUE, show.legend=TRUE)
  @ 
  \end{column}
\end{columns}
}

