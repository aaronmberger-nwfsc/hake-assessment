%----------------------------------------------------------
     \frame{\frametitle{Time-varying selectivity}
       \begin{columns}
       \begin{column}{0.48\textwidth}
       \vspace{-10pt}
       \bc
       <<main-tv-selex, fig.height=12, fig.width=10,out.width='0.8\\columnwidth'>>=
         ## Extract TV selectivity fits by running the calculation function.
         tv_selex_start_yr <- 1990
         tv_selex_end_yr <- last_data_yr
         tv_selex_ages <- as.character(1:8)
         tv_selex_med_raw <- base_model$extra.mcmc$sel_fishery_median
         tv_selex_med_filt <- tv_selex_med_raw |>
           filter(Yr %in% tv_selex_start_yr:tv_selex_end_yr)
         tv_selex_med_yrs <- tv_selex_med_filt |> pull(Yr)
         tv_selex_med_mat <- tv_selex_med_filt |>
           select(!!!tv_selex_ages) |>
           as.matrix()
         tv_selex_lower <- base_model$extra.mcmc$sel_fishery_lower
         tv_selex_upper <- base_model$extra.mcmc$sel_fishery_upper
         oldpar <- par()
         # padding left and right outer margins using oma to make plot less stretched out
         par(mar = c(4, 4, 1, 1),
             oma = c(0, 8, 0, 8))
         mountains(tv_selex_med_mat,
                   yvec = tv_selex_med_yrs,
                   rev = TRUE,
                   cex.axis = 0.8)
         mtext(side = 1, line = 2, "Age")
         mtext(side = 2, line = 3, "Selectivity by year")
         par <- oldpar
         @
           \ec
         \end{column}
         \begin{column}{0.48\textwidth}
         \vspace{-10pt}
         \bc
         <<main-tv-selex-uncertainty, fig.height=12, fig.width=10, out.width='0.8\\columnwidth'>>=
         plot_multiple_tv_selex_unc(base_model, n_col = 2)
         @
           \ec
         \end{column}
         \end{columns}
     }

