<<setup, echo=FALSE>>=
# All the model results to show diagnostics for:
# Not automatic, but makes it easy to add/leave any out
diagnostic.model <- c("base.model",
                      "sens.models.1[[1]]",
                      "sens.models.1[[2]]",
                      "sens.models.1[[3]]",
                      "sens.models.1[[4]]",
                      "sens.models.1[[5]]",
                      "sens.models.1[[6]]",
                      "sens.models.1[[7]]",
                      "sens.models.2[[1]]",
                      "sens.models.2[[2]]",
                      "sens.models.2[[3]]",
                      "sens.models.4[[1]]",
                      "sens.models.4[[2]]",
                      "sens.models.4[[3]]",
                      "sens.models.4[[4]]",
                      "sens.models.5"     # only one element
                      )
@

<<generate, echo=FALSE, results='asis'>>=
# This duplicates the ..one-model.rnw code with {{this.sens.model}} replaced
#  with each element of diagnostic.model in turn:
generated <- lapply(diagnostic.model,
                    function(this.sens.model){
  knitr::knit_expand('appendix-mcmc-diagnostics/adnuts-diagnostics-one-model.rnw')
})
res <- knitr::knit_child(text = unlist(generated),
                         quiet = TRUE)
cat(res, sep = '\n')
@


